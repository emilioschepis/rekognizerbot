{
  "name": "rekognizerbot",
  "version": "1.0.0",
  "repository": "https://github.com/emilioschepis/rekognizerbot",
  "author": "Emilio Schepis",
  "description": "Serverless Telegram Bot",
  "main": "handler.ts",
  "scripts": {
    "deploy": "serverless deploy",
    "deploy:bot": "serverless deploy -f bot",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint . --ext .ts,.tsx"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@aws-sdk/client-rekognition-node": "0.1.0-preview.6",
    "axios": "^0.19.2",
    "source-map-support": "^0.5.10",
    "telegraf": "^3.37.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/aws-lambda": "^8.10.46",
    "@types/jest": "^25.1.4",
    "@types/node": "^10.17.18",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-jest": "^25.2.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^25.2.4",
    "prettier": "^1.19.1",
    "serverless-webpack": "^5.3.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-node-externals": "^1.7.2"
  },
  "license": "MIT"
}
